<div class="form-group nested-fields">
  <div class='form-row'>
    <div class='col-md-4 mb-3'>
      <%= f.label :vaccination %>
      <%= f.text_field :name, class: 'form-control' %>
    </div>
    <div class="col-md-4 mb-3">
      <%= f.label :received %>
      <%= f.date_field :rec, class: 'form-control' %>
    </div>
    <div class="col-md-4 mb-3">
      <%= f.label :expires %>
      <%= f.date_field :exp, class: 'form-control' %>
    </div>
    <%= link_to_remove_association 'Remove Vaccination', f, class: 'btn btn-outline-danger btn-sm' %>
  </div>
</div>